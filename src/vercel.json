{
    "version":2,
    "builds":[
         { 
               "src":"src/main.ts",
                "use":"@vercel/node",
                "config": {
                    "outputDirectory": "dist"}
        }    
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "method":[
                "GET", "POST","PUT","DELETE","PATCH" 
            ]
        }

    ]
}